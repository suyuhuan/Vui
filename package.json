{
  "name": "vui",
  "version": "1.0.0",
  "description": "这是一个vue开发的UI框架",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/parcel index.html",
    "dev-test": "parcel watch test/* --no-cache & karma start",
    "eg-test": "parcel build  test/* --no-minify & karma start --single-run",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name gulu src/index.js",
    "test": "cross-env BABEL_ENV=test karma start --single-run",
    "test:unit": "cross-env BABEL_ENV=test karma start",
    "test:unit:old": "vue-cli-service test:unit",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suyuhuan/Vui.git"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "SUYUHUAN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suyuhuan/Vui/issues"
  },
  "homepage": "https://github.com/suyuhuan/Vui#readme",
  "dependencies": {
    "@vue/test-utils": "^1.0.3",
    "char": "^1.0.2",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.2",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^5.0.9",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^7.2.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  }
}
